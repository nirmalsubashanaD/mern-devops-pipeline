{
    "name": "backend",
    "version": "1.0.0",
    "main": "server.js",
    "scripts": {
      "test": "jest --coverage"
    },
    "devDependencies": {
      "jest": "^30.0.0-beta.3",
      "jest-junit": "^16.0.0",
      "supertest": "^7.1.1"
    },
    "dependencies": {
      "express": "^5.1.0"
    },
    "jest": {
  "reporters": [
    "default",
    [
      "jest-junit",
      {
        "outputDirectory": "test-results",
        "outputName": "results.xml"
      }
    ]
  ],
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": 80
    }
  }
}

  }
  
